pipeline{
    agent any
    stages{
        stage("Checkout Code"){
            steps{
                git branche 'main' url: git@github.com:YannickGongue/BankApp.git'
                }         
        }
        stage("Build C# App"){
            steps{
                sh '''dotnet restore BankApp.sln
                dotnet build BankApp.sln -c Release
                '''
            }
        }
    }
    
}   
